{
  "extends": [
    "colinrcummings",
    "colinrcummings/jest",
    "colinrcummings/prettier"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "import/no-unresolved": [
      "error",
      {
        "ignore": [
          "@testing-library/jest-dom/extend-expect",
          "@testing-library/react"
        ]
      }
    ]
  }
}
